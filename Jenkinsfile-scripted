
pipeline {
  agent any
  environment {
      branch = "${env.GIT_BRANCH}"
  }
  stages {
    stage("build") {
      steps {
        echo "Build stage"
        echo "branch name iiis ${branch} "
        script {
            def example = load "example.groovy"
            example1 = example.exampleMethod()
        }
      }
    }
    stage("Test") {
        when {
            expression {
                "${env.BRANCH_NAME}" == 'origin/master'
            }
        }
      steps {
        echo "Test stage"
      }
    }
  }
}
