
pipeline {
  agent any
  environment {
      node = env.NODE_NAME
  }
  stages {
    stage("build") {
      steps {
        echo "Build stage"
        echo "branch name iiis ${node} "
        script {
            def example = load "example.groovy"
            example1 = example.exampleMethod()
        }
      }
    }
    stage("Test") {
        when {
            expression {
                env.BRANCH_NAME == 'master'
            }
        }
      steps {
        echo "Test stage"
      }
    }
  }
}
