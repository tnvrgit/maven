pipeline {
  agent any
  stages {
    stage("build") {
      steps {
        echo "Build stage"
        script {
            def example = load "example.groovy"
            example1 = example.exampleMethod()
        }
      }
    }
    stage("Test") {
        when {
            expression {
                BRANCH_NAME == 'master'
            }
        }
      steps {
        echo "Test stage"
      }
    }
  }
}
