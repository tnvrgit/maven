@Library('shared-lib') _ 

pipeline{

      agent any 
        environment {
            branch = "${env.GIT_BRANCH}"
        }
        stages {
         stage('maven build'){
                  steps{
                      script{
		    	                sh "mvn clean install"
                      	  }
               	     }  
                 }	
                 
                 stage ('Check logs') {
                    steps {
                        filterLogs ('WARNING', 1)
                    }
                }
          }
   	     	         
}